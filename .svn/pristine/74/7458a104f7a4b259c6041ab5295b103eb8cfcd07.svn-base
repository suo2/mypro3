                            android:id="@+id/tv_line_power_13"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="5dp"
                            android:layout_weight="11"
                            android:textColor="#333333"
                            android:textSize="12sp" />
                    </LinearLayout>

