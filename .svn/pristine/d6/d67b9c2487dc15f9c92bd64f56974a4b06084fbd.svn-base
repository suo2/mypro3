<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="topView">
        <attr name="topView_stroke_width" format="dimension"></attr>
        <attr name="topView_color" format="color"></attr>
    </declare-styleable>

    <declare-styleable name="menu_cover_View">
        <attr name="menu_cover_radius" format="dimension"></attr>
        <attr name="menu_cover_color" format="color"></attr>
    </declare-styleable>
    <declare-styleable name="ArrowTextView">
        <attr name="radiuss" format="dimension" />
        <attr name="arrowWidth" format="dimension" />
        <attr name="arrowHeight" format="dimension" />
        <attr name="bg" format="color" />
    </declare-styleable>
    <declare-styleable name="FloatingActionMenu">
        <attr name="base_src" format="reference" />
        <attr name="base_background" format="color" />
        <attr name="base_ripple" format="color" />
        <attr name="base_marginEnd" format="dimension" />
        <attr name="base_marginBottom" format="dimension" />
        <attr name="overlay_color" format="color" />
        <attr name="item_spacing" format="dimension" />
        <attr name="enable_labels" format="boolean" />
        <attr name="overlay_duration" format="integer" />
        <attr name="label_background" format="color" />
        <attr name="label_fontSize" format="float" />
        <attr name="label_fontColor" format="color" />
        <attr name="label_marginEnd" format="dimension" />
        <attr name="actions_duration" format="integer" />
    </declare-styleable>

    <declare-styleable name="AllAngleExpandableButton">
        <attr name="aebMainButtonSizeDp" format="dimension|reference" />
        <attr name="aebSubButtonSizeDp" format="dimension|reference" />
        <attr name="aebButtonGapDp" format="dimension|reference" />
        <attr name="aebStartAngleDegree" format="integer|reference" />
        <attr name="aebEndAngleDegree" format="integer|reference" />
        <attr name="aebAnimDurationMillis" format="integer|reference" />
        <attr name="aebButtonElevation" format="dimension|reference" />
        <attr name="aebMaskBackgroundColor" format="color|reference" />
        <attr name="aebIsSelectionMode" format="boolean" />
        <attr name="aebMainButtonTextColor" format="color|reference" />
        <attr name="aebSubButtonTextColor" format="color|reference" />
        <attr name="aebMainButtonTextSizeSp" format="dimension|reference" />
        <attr name="aebSubButtonTextSizeSp" format="dimension|reference" />
        <attr name="aebRippleEffect" format="boolean" />
        <attr name="aebRippleColor" format="color|reference" />
        <attr name="aebMainButtonRotateDegree" format="integer|reference" />
        <attr name="aebMainButtonRotateAnimDurationMillis" format="integer|reference" />
        <attr name="aebBlurBackground" format="boolean" />
        <attr name="aebBlurRadius" format="float|reference" />
    </declare-styleable>

    <declare-styleable name="CircleImageView">
        <attr name="shape" format="integer">
            <enum name="circle" value="1" />
            <enum name="rectangle" value="2" />
            <enum name="svg" value="3" />
        </attr>
        <attr name="svg_raw_resource" format="reference" />
    </declare-styleable>

    <declare-styleable name="CircularFloatingMenu">
        <attr name="radius" format="reference|dimension" />
        <attr name="degrees" format="reference|integer" />
        <attr name="startDegrees" format="reference|integer" />
        <attr name="isCloseWhenItemClick" format="boolean" />
    </declare-styleable>

    <attr name="firstColor" format="color" />
    <attr name="secondColor" format="color" />
    <attr name="thirdColor" format="color" />
    <attr name="circleWidth" format="dimension" />
    <attr name="defaultBg" format="color" />
    <declare-styleable name="CustomProgressBar">
        <attr name="firstColor" />
        <attr name="secondColor" />
        <attr name="thirdColor" />
        <attr name="circleWidth" />
        <attr name="defaultBg" />
    </declare-styleable>
    <declare-styleable name="HorizontalProgress">
        <attr name="hpWidth" format="dimension" />
        <attr name="hpHeight" format="dimension" />
        <attr name="hpTextMarginLeft" format="dimension" />
        <attr name="hpTextSize" format="dimension" />
    </declare-styleable>
    <declare-styleable name="CoverFlowView">
        <attr name="visibleViews" format="integer" />
        <attr name="coverflowGravity" format="enum">
            <enum name="top" value="0" />
            <enum name="bottom" value="1" />
            <enum name="center_vertical" value="2" />
        </attr>
        <attr name="coverflowLayoutMode" format="enum">
            <enum name="match_parent" value="0" />
            <enum name="wrap_content" value="1" />
        </attr>
    </declare-styleable>
    <declare-styleable name="FlowLayout">
        <attr name="android:orientation" />
        <attr name="layoutDirection" format="enum">
            <enum name="ltr" value="0" />
            <enum name="rtl" value="1" />
        </attr>
        <attr name="debugDraw" format="boolean" />
        <attr name="weightDefault" format="float" />
        <attr name="android:gravity" />
    </declare-styleable>
    <declare-styleable name="FlowLayout_LayoutParams">
        <attr name="layout_newLine" format="boolean" />
        <attr name="layout_weight" format="float" />
        <attr name="android:layout_gravity" />
    </declare-styleable>
    <declare-styleable name="CircleImage">
        <attr name="civ_border_width" format="dimension" />
        <attr name="civ_border_color" format="color" />
        <attr name="civ_border_overlay" format="boolean" />
        <attr name="civ_fill_color" format="color" />
    </declare-styleable>

    <declare-styleable name="FlowLineView">
        <attr name="flowLineColor" format="color" />
        <attr name="line_dirction" format="string" />
        <attr name="flow_dirction" format="string" />
    </declare-styleable>

    <declare-styleable name="PercentCircleView">
        <attr name="first_circle_color" format="color" />
        <attr name="second_circle_color" format="color" />
        <attr name="base_bottom_color" format="color" />
        <attr name="cicle_radius" format="dimension" />
        <attr name="stroke_width" format="dimension" />
        <attr name="text_size" format="dimension" />
    </declare-styleable>
    <declare-styleable name="QuickSideBarView">
        <attr name="sidebarTextColor" format="color|reference"/>
        <attr name="sidebarBackgroundColor" format="color|reference"/>
        <attr name="sidebarTextColorChoose" format="color|reference"/>
        <attr name="sidebarTextSize" format="dimension"/>
        <attr name="sidebarTextSizeChoose" format="dimension"/>
        <attr name="sidebarItemHeight" format="dimension"/>
    </declare-styleable>


    <declare-styleable name="MyLinearLayout">
        <attr name="titleOfMyLinearLayout" format="string"/>
        <attr name="dividerShowOfMyLinearLayout" format="boolean"/>
        <attr name="expandedOfMyLinearLayout" format="boolean"/>
        <attr name="contentOfMyLinearLayout" format="string"/>
        <attr name="decoratedDrawableOfMyLinearLayout" format="reference"/>
        <attr name="formNameOfMyLinearLayout" format="reference"/>
        <attr name="isNumOfMyLinearLayout" format="boolean"/>
        <attr name="isEditText" format="boolean"/>
        <attr name="isPsw" format="boolean"/>
    </declare-styleable>

    <!--自定义时间选择器-->
    <declare-styleable name="pickerview">
        <attr name="pickerview_gravity">
            <enum name="center" value="17"/>
            <enum name="left" value="3"/>
            <enum name="right" value="5"/>
        </attr>
        <attr name="pickerview_textSize" format="dimension"/>
        <attr name="pickerview_textColorOut" format="color"/>
        <attr name="pickerview_textColorCenter" format="color"/>
        <attr name="pickerview_dividerColor" format="color"/>
        <attr name="pickerview_lineSpacingMultiplier" format="float"/>
    </declare-styleable>

    <!--字体大小自适应文本框-->
    <declare-styleable name="AutofitTextView">
        <!-- Minimum size of the text. -->
        <attr name="minTextSize" format="dimension" />
        <!-- Amount of precision used to calculate the correct text size to fit within its
        bounds. Lower precision is more precise and takes more time. -->
        <attr name="precision" format="float" />
        <!-- Defines whether to automatically resize text to fit to the view's bounds. -->
        <attr name="sizeToFit" format="boolean" />
    </declare-styleable>
    <declare-styleable name="SwipeLayout">
    <attr name="drag_edge">
        <flag name="left" value="1" />
        <flag name="right" value="2" />
        <flag name="top" value="4" />
        <flag name="bottom" value="8" />
    </attr>
    <attr name="leftEdgeSwipeOffset" format="dimension" />
    <attr name="rightEdgeSwipeOffset" format="dimension" />
    <attr name="topEdgeSwipeOffset" format="dimension" />
    <attr name="bottomEdgeSwipeOffset" format="dimension" />
    <attr name="show_mode" format="enum">
        <enum name="lay_down" value="0" />
        <enum name="pull_out" value="1" />
    </attr>
    <attr name="clickToClose" format="boolean" />
</declare-styleable>

</resources>