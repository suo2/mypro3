<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:padding="@dimen/size_10dp"
    android:orientation="vertical">

    <!-- 220/110KV母线/分段间隔电路图 -->


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <View
            android:id="@+id/line1"
            android:layout_width="0dp"
            android:layout_height="@dimen/booster_wide_line"
            android:layout_weight="2"
            android:background="@color/kv_110"/>

        <View
            android:layout_width="0dp"
            android:layout_height="5dp"
            android:layout_weight="1" />
        <View
            android:id="@+id/line2"
            android:layout_width="0dp"
            android:layout_height="@dimen/booster_wide_line"
            android:layout_weight="2"
            android:background="@color/kv_110"/>


    </LinearLayout>


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <LinearLayout
            android:layout_width="0dp"
            android:layout_weight="2"
            android:layout_height="wrap_content"
            android:orientation="vertical">


            <View
                android:id="@+id/line3"
                android:layout_width="@dimen/booster_narrow_line"
                android:layout_height="100dp"
                android:layout_gravity="center_horizontal"
                android:background="@color/kv_110"/>


            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <ImageView
                    android:id="@+id/switch_15900"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerInParent="true"
                    android:src="@drawable/vertical_switch_off"/>

                <TextView
                    android:id="@+id/tx_15900"
                    style="@style/tx_booster_details"
                    android:layout_centerVertical="true"
                    android:layout_toLeftOf="@+id/switch_15900"
                    android:layout_marginRight="5dp"
                    android:text="15900"
                    />

            </RelativeLayout>
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="100dp">

                <View
                    android:id="@+id/line4"
                    android:layout_width="@dimen/booster_narrow_line"
                    android:layout_height="100dp"
                    android:layout_centerHorizontal="true"
                    android:background="@color/kv_110"/>

                <LinearLayout
                    android:id="@+id/switch_linear"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical"
                    android:layout_centerVertical="true"
                    android:layout_toRightOf="@+id/line4"
                    android:orientation="horizontal">

                    <View
                        android:id="@+id/line5"
                        android:layout_width="20dp"
                        android:background="@color/kv_110"
                        android:layout_height="@dimen/booster_narrow_line"/>


                    <ImageView
                        android:id="@+id/switch_15903"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:paddingBottom="2dp"
                        android:src="@drawable/horizontal_switch_on"/>

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@drawable/horizontal_ground"/>

                </LinearLayout>

                <TextView
                    android:id="@+id/tx_15903"
                    style="@style/tx_booster_details"
                    android:layout_below="@+id/switch_linear"
                    android:layout_marginLeft="30dp"
                    android:layout_toRightOf="@+id/line4"
                    android:text="15905"
                    />

            </RelativeLayout>

            <View
                android:id="@+id/line6"
                android:layout_width="@dimen/booster_narrow_line"
                android:layout_height="50dp"
                android:layout_gravity="center_horizontal"
                android:background="@color/kv_110"/>


        </LinearLayout>

        <View
            android:layout_width="0dp"
            android:layout_height="5dp"
            android:layout_weight="1" />


        <LinearLayout
            android:layout_width="0dp"
            android:layout_weight="2"
            android:layout_height="wrap_content"
            android:orientation="vertical">


            <View
                android:id="@+id/line7"
                android:layout_width="@dimen/booster_narrow_line"
                android:layout_height="100dp"
                android:layout_gravity="center_horizontal"
                android:background="@color/kv_110"/>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <ImageView
                    android:id="@+id/switch_15901"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerInParent="true"
                    android:src="@drawable/vertical_switch_off"/>

                <TextView
                    android:id="@+id/tx_15901"
                    style="@style/tx_booster_details"
                    android:layout_centerVertical="true"
                    android:layout_toLeftOf="@+id/switch_15901"
                    android:layout_marginRight="5dp"
                    android:text="15900"
                    />

            </RelativeLayout>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="100dp">

                <View
                    android:id="@+id/line8"
                    android:layout_width="@dimen/booster_narrow_line"
                    android:layout_height="100dp"
                    android:layout_centerHorizontal="true"
                    android:background="@color/kv_110"/>

                <LinearLayout
                    android:id="@+id/switch_15905_ll"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical"
                    android:layout_centerVertical="true"
                    android:layout_toRightOf="@+id/line8"
                    android:orientation="horizontal">

                    <View
                        android:id="@+id/line9"
                        android:layout_width="20dp"
                        android:background="@color/kv_110"
                        android:layout_height="@dimen/booster_narrow_line"/>


                    <ImageView
                        android:id="@+id/switch_15905"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:paddingBottom="2dp"
                        android:src="@drawable/horizontal_switch_on"/>

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@drawable/horizontal_ground"/>

                </LinearLayout>

                <TextView
                    android:id="@+id/tx_15905"
                    style="@style/tx_booster_details"
                    android:layout_below="@+id/switch_15905_ll"
                    android:layout_marginLeft="30dp"
                    android:layout_toRightOf="@+id/line8"
                    android:text="15905"
                    />

            </RelativeLayout>

            <View
                android:id="@+id/line10"
                android:layout_width="@dimen/booster_narrow_line"
                android:layout_height="50dp"
                android:layout_gravity="center_horizontal"
                android:background="@color/kv_110"/>


        </LinearLayout>


    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:orientation="horizontal">


        <RelativeLayout
            android:layout_width="0dp"
            android:layout_height="31dp"
            android:layout_weight="1" >

            <View
                android:id="@+id/line11"
                android:layout_width="1dp"
                android:layout_height="31dp"
                android:layout_alignParentRight="true"
                android:background="@color/kv_110"/>


        </RelativeLayout>

        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="3"
            android:orientation="horizontal">

            <View
                android:id="@+id/line12"
                android:layout_width="1dp"
                android:layout_height="31dp"
                android:background="@color/kv_110"/>
            <View
                android:id="@+id/line13"
                android:layout_width="0dp"
                android:layout_height="@dimen/booster_narrow_line"
                android:layout_weight="1"
                android:layout_gravity="center_vertical"
                android:background="@color/kv_110"/>

            <RelativeLayout
                android:layout_width="wrap_content"
                android:layout_height="match_parent">

                <ImageView
                    android:id="@+id/circuit_breaker_15902"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/circuit_breaker_close_horizontal"
                    android:layout_centerInParent="true"/>

                <TextView
                    android:id="@+id/tx_15902"
                    style="@style/tx_booster_details"
                    android:layout_below="@+id/circuit_breaker_15902"
                    android:layout_marginTop="@dimen/size_5dp"
                    android:layout_centerHorizontal="true"
                    android:text="15902" />


            </RelativeLayout>

            <View
                android:id="@+id/line14"
                android:layout_width="0dp"
                android:layout_height="@dimen/booster_narrow_line"
                android:layout_weight="1"
                android:layout_gravity="center_vertical"
                android:background="@color/kv_110"/>

            <View
                android:id="@+id/line15"
                android:layout_width="1dp"
                android:layout_height="31dp"
                android:background="@color/kv_110"/>

        </LinearLayout>

        <RelativeLayout
            android:layout_width="0dp"
            android:layout_height="31dp"
            android:layout_weight="1" >

            <View
                android:id="@+id/line16"
                android:layout_width="1dp"
                android:layout_height="31dp"
                android:background="@color/kv_110"/>


        </RelativeLayout>



    </LinearLayout>




</LinearLayout>
