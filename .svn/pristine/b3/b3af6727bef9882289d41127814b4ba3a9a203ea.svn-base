<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/common_white"
    android:orientation="vertical"
    tools:context="com.huawei.solarsafe.view.devicemanagement.HouseholdDataSettingActivity">

    <LinearLayout
        android:id="@+id/ll_1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <RelativeLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1">

            <TextView
                android:id="@+id/jindu1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true"
                android:layout_marginBottom="8dp"
                android:layout_marginTop="16dp"
                android:background="@drawable/jinduliang"
                android:gravity="center"
                android:text="@string/one"
                android:textColor="@color/common_white" />

            <View
                android:id="@+id/line_jindu1"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_centerVertical="true"
                android:layout_toRightOf="@id/jindu1"
                android:background="@color/jindu_line_status_2" />
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1">

            <TextView
                android:id="@+id/jindu2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true"
                android:layout_marginBottom="8dp"
                android:layout_marginTop="16dp"
                android:background="@drawable/jinduhui"
                android:gravity="center"
                android:text="@string/two"
                android:textColor="@color/common_white" />

            <View
                android:id="@+id/line_jindu2"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_centerVertical="true"
                android:layout_toRightOf="@id/jindu2"
                android:background="@color/jindu_line_status_2" />

            <View
                android:id="@+id/line_jindu11"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_centerVertical="true"
                android:layout_toLeftOf="@id/jindu2"
                android:background="@color/jindu_line_status_2" />
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1">

            <TextView
                android:id="@+id/jindu3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true"
                android:layout_marginBottom="8dp"
                android:layout_marginTop="16dp"
                android:background="@drawable/jinduhui"
                android:gravity="center"
                android:text="@string/three"
                android:textColor="@color/common_white" />

            <View
                android:id="@+id/line_jindu3"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_centerVertical="true"
                android:layout_toRightOf="@id/jindu3"
                android:background="@color/jindu_line_status_2" />

            <View
                android:id="@+id/line_jindu22"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_centerVertical="true"
                android:layout_toLeftOf="@id/jindu3"
                android:background="@color/jindu_line_status_2" />
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1">

            <TextView
                android:id="@+id/jindu4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true"
                android:layout_marginBottom="8dp"
                android:layout_marginTop="16dp"
                android:background="@drawable/jinduhui"
                android:gravity="center"
                android:text="@string/four"
                android:textColor="@color/common_white" />

            <View
                android:id="@+id/line_jindu4"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_centerVertical="true"
                android:layout_toRightOf="@id/jindu4"
                android:background="@color/jindu_line_status_2"
                android:visibility="gone" />

            <View
                android:id="@+id/line_jindu33"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_centerVertical="true"
                android:layout_toLeftOf="@id/jindu4"
                android:background="@color/jindu_line_status_2" />
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:visibility="gone">

            <TextView
                android:id="@+id/jindu5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true"
                android:layout_marginBottom="8dp"
                android:layout_marginTop="16dp"
                android:background="@drawable/jinduhui"
                android:gravity="center"
                android:text="@string/five"
                android:textColor="@color/common_white" />

            <View
                android:id="@+id/line_jindu5"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_centerVertical="true"
                android:layout_toRightOf="@id/jindu5"
                android:background="@color/jindu_line_status_2" />

            <View
                android:id="@+id/line_jindu44"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_centerVertical="true"
                android:layout_toLeftOf="@id/jindu5"
                android:background="@color/jindu_line_status_2" />
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:visibility="gone">

            <TextView
                android:id="@+id/jindu6"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true"
                android:layout_marginBottom="8dp"
                android:layout_marginTop="16dp"
                android:background="@drawable/jinduhui"
                android:gravity="center"
                android:text="@string/six"
                android:textColor="@color/common_white" />

            <View
                android:id="@+id/line_jindu55"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_centerVertical="true"
                android:layout_toLeftOf="@id/jindu6"
                android:background="@color/jindu_line_status_2" />
        </RelativeLayout>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/ll_2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/ll_1"
        android:layout_marginBottom="8dp"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/jindu_name_1"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:gravity="center"
            android:text="@string/electrical_parameter"
            android:textColor="@color/jindu_line_status_1"
            android:textSize="11sp" />

        <TextView
            android:id="@+id/jindu_name_2"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:gravity="center"
            android:text="@string/protection_parameters"
            android:textColor="@color/jindu_line_status_2"
            android:textSize="11sp" />

        <TextView
            android:id="@+id/jindu_name_3"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:gravity="center"
            android:text="@string/parameter_property"
            android:textColor="@color/jindu_line_status_2"
            android:textSize="11sp" />

        <TextView
            android:id="@+id/jindu_name_4"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:gravity="center"
            android:text="@string/power_regulation"
            android:textColor="@color/jindu_line_status_2"
            android:textSize="11sp" />

        <TextView
            android:id="@+id/jindu_name_5"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:gravity="center"
            android:text="@string/timeset"
            android:textColor="@color/jindu_line_status_2"
            android:textSize="11sp"
            android:visibility="gone" />

        <TextView
            android:id="@+id/jindu_name_6"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:gravity="center"
            android:text="@string/communications_parameters"
            android:textColor="@color/jindu_line_status_2"
            android:textSize="11sp"
            android:visibility="gone" />
    </LinearLayout>

    <View
        android:id="@+id/view_1"
        android:layout_width="match_parent"
        android:layout_height="10dp"
        android:layout_below="@id/ll_2"
        android:background="#eeeeee" />

    <FrameLayout
        android:id="@+id/fragment_container"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@id/view_1"
        android:layout_marginBottom="50dp" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true">

        <Button
            android:id="@+id/btn_before"
            android:layout_width="0dp"
            android:layout_height="40dp"
            android:layout_marginBottom="5dp"
            android:layout_marginLeft="15dp"
            android:layout_marginTop="5dp"
            android:layout_weight="1"
            android:layout_gravity="center_vertical"
            android:background="@drawable/shape_single_item_four_corner_empty"
            android:text="@string/cancel_"
            android:textSize="14sp" />

        <Button
            android:id="@+id/btn_yjsz"
            android:layout_width="0dp"
            android:layout_height="40dp"
            android:layout_marginBottom="5dp"
            android:layout_marginLeft="15dp"
            android:layout_marginTop="5dp"
            android:layout_weight="1"
            android:layout_gravity="center_vertical"
            android:background="@drawable/btn_pressed_selector_full_yellow"
            android:text="@string/key_set"
            android:textColor="@color/common_white"
            android:textSize="14sp" />

        <Button
            android:id="@+id/btn_next"
            android:layout_width="0dp"
            android:layout_height="40dp"
            android:layout_marginBottom="5dp"
            android:layout_marginLeft="15dp"
            android:layout_marginRight="15dp"
            android:layout_marginTop="5dp"
            android:layout_gravity="center_vertical"
            android:layout_weight="1"
            android:background="@drawable/btn_pressed_selector_full_yellow"
            android:text="@string/next"
            android:textColor="@color/common_white"
            android:textSize="14sp" />
    </LinearLayout>
</RelativeLayout>
