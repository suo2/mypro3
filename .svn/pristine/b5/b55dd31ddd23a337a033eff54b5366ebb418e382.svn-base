<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- 新建电站-顶部进度-LinearLayout-->
    <style name="new_station_progress_llyt_style">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">1</item>
        <item name="android:orientation">vertical</item>
    </style>
    <!-- 新建电站-顶部进度-top-LinearLayout-->
    <style name="new_station_progress_top_llyt_style">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:orientation">horizontal</item>
    </style>
    <!-- 新建电站-顶部进度-step-TextView-->
    <style name="new_station_progress_tv_style">
        <item name="android:layout_width">24dp</item>
        <item name="android:layout_height">24dp</item>
        <item name="android:background">@drawable/new_station_progress_tv_bg_selector</item>
        <item name="android:layout_gravity">center_vertical|center_horizontal</item>
        <item name="android:gravity">center_horizontal|center_vertical</item>
        <item name="android:textColor">@color/white</item>
    </style>
    <!-- 新建电站-顶部进度-分割线View-->
    <style name="new_station_progress_line_style">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">1dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:background">@drawable/new_station_progress_line_bg_selector</item>
        <item name="android:layout_gravity">center_vertical</item>
    </style>
    <!-- 新建电站-顶部进度-描述-TextView-->
    <style name="new_station_progress_dscribe_tv_style">
        <item name="android:layout_marginTop">5dp</item>
        <item name="android:textSize">10sp</item>>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">center_vertical|center_horizontal</item>
        <item name="android:gravity">center_horizontal|center_vertical</item>
        <item name="android:textColor">@color/new_station_progress_txt_color_selector</item>
    </style>
    <!-- 新建电站-底部操作黄色Button-->
    <style name="new_station_operation_yellow_btn_style">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">40dp</item>
        <item name="android:layout_margin">8dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:background">@drawable/btn_pressed_selector_full_yellow</item>
        <item name="android:textColor">@color/common_white</item>
        <item name="android:textSize">11sp</item>
    </style>
    <!-- 新建电站-底部操作白色Button-->
    <style name="new_station_operation_whilte_btn_style" parent="new_station_operation_yellow_btn_style">
        <item name="android:background">@drawable/shape_single_item_four_corner_empty</item>
        <item name="android:textColor">#333333</item>
    </style>
    <!-- 新建电站-内容条目-当行最外层容器-LinearLayout -->
    <style name="new_station_item_llyt">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:paddingLeft">10dp</item>
        <item name="android:paddingRight">10dp</item>
        <item name="android:paddingTop">5dp</item>
        <item name="android:paddingBottom">5dp</item>
    </style>
    <!-- 新建电站-内容条目-左侧名称-TextView -->
    <style name="new_station_item_tv_name">
        <item name="android:layout_width">80dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">12sp</item>
        <item name="android:textColor">#262626</item>
    </style>
    <!-- 新建电站-内容条目-右侧容器父类-->
    <style name="new_station_item_contain_parent">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_weight">3.5</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:paddingTop">5dp</item>
        <item name="android:paddingBottom">5dp</item>
        <item name="android:gravity">center_vertical</item>
    </style>
    <!-- 新建电站-内容条目-右侧输入框-EditText -->
    <style name="new_station_item_et_content" parent="@style/new_station_item_contain_parent">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">12sp</item>
        <item name="android:textColorHint">@color/text_three</item>
        <item name="android:textColor">#0D0D0D</item>
        <item name="android:background">@null</item>
        <item name="layout_weight">1</item>
    </style>
    <!-- 电站管理-输入框-EditText -->
    <style name="station_item_et_content" parent="@style/new_station_item_contain_parent">
        <item name="android:textSize">12sp</item>
        <item name="android:textColor">#333</item>
        <item name="android:paddingLeft">5dp</item>
        <item name="android:paddingRight">5dp</item>
        <item name="android:background">@drawable/new_station_photo_bg</item>
    </style>
    <!-- 电站管理-经纬度输入框-EditText -->
    <style name="station_item_et_background" >
        <item name="android:textSize">15sp</item>
        <item name="android:textColor">#333</item>
        <item name="android:paddingLeft">5dp</item>
        <item name="android:paddingRight">5dp</item>
        <item name="android:paddingTop">5dp</item>
        <item name="android:paddingBottom">5dp</item>
        <item name="android:background">@drawable/new_station_photo_bg</item>
    </style>

    <!-- 新建电站-内容条目-右侧输入框位置-容器-RelativeLayout -->
    <style name="new_station_item_rlyt_content" parent="@style/new_station_item_contain_parent"></style>
    <!-- 新建电站-内容条目-右侧输入框位置-容器内输入框-EditText -->
    <style name="new_station_item_contain_et_content">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">@color/danzhan_color</item>
        <item name="android:background">@null</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:maxLines">2</item>
    </style>
    <!-- 新建电站-内容条目-右侧输入框位置-容器内下拉选择框-Spinner -->
    <style name="new_station_item_contain_sp_content">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:textSize">12sp</item>
        <item name="android:textColor">#333</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:background">@null</item>
    </style>
    <!-- 新建电站-内容条目-右侧输入框位置-容器内右侧单位-EditText -->
    <style name="new_station_item_contain_tv_unit">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">15sp</item>
        <item name="android:textColor">#333</item>
        <item name="android:layout_alignParentRight">true</item>
        <item name="android:layout_centerVertical">true</item>
    </style>
    <!-- 新建电站-内容条目-右侧输入框位置-容器内右侧下拉箭头-ImageView -->
    <style name="new_station_item_contain_iv_drop">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:src">@drawable/spinner_down_arrow</item>
        <item name="android:layout_alignParentRight">true</item>
        <item name="android:layout_centerVertical">true</item>
    </style>
</resources>