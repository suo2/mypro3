<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.huawei.solarsafe.view.pnlogger.ConnectStationActivity">

    <!--头部-->
    <include layout="@layout/common_head_shucai" />

    <View
        style="@style/horizontal_divider"
        android:layout_marginBottom="9dp" />

    <!--引导部分-->
    <include layout="@layout/layout_station_selected" />

    <View style="@style/horizontal_divider" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="74dp"
        android:layout_gravity="center_horizontal"
        android:src="@drawable/sjtb" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:layout_marginBottom="22dp"
        android:layout_marginTop="13dp"
        android:text="@string/data_sync_complete"
        android:textColor="@color/black"
        android:textSize="17sp" />


    <View
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:layout_weight="1" />

    <Button
        android:id="@+id/btn_conn_finish"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_marginBottom="10dp"
        android:textColor="@color/shucai_color_on"
        android:textSize="17sp"
        android:text="@string/complete_connect_back_home" />
</LinearLayout>
