package com.huawei.solarsafe.bean.common;

import com.huawei.solarsafe.bean.station.kpi.StationList;

/**
 * Created by P00709 on 2018/4/25.
 */

public class ResetBean {

    private StationList stationInfos;
    private String needReset;
    private String reason ;

    public ResetBean() {
    }

    public ResetBean(StationList stationInfos, String needReset, String reason) {
        this.stationInfos = stationInfos;
        this.needReset = needReset;
        this.reason = reason;
    }

    public StationList getStationInfos() {
        return stationInfos;
    }

    public void setStationInfos(StationList stationInfos) {
        this.stationInfos = stationInfos;
    }

    public String getNeedReset() {
        return needReset;
    }

    public void setNeedReset(String needReset) {
        this.needReset = needReset;
    }

    public String getReason() {
        return reason;
    }

    public void setReason(String reason) {
        this.reason = reason;
    }
}
