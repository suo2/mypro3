package com.huawei.solarsafe.model.maintain;

/**
 * 移动运维 流程相关常量
 * Created by P00319 on 2017/3/9.
 */

public interface IProcState {

    String DEFECT = "defect";
    String INSPECT = "inspect";


    //缺陷流程状态
    /**
     * 消缺中
     */
    String DEFECT_HANDLING = "defectHandle";
    /**
     * 已退回/待分配
     */
    String DEFECT_WRITE = "defectWrite";
    /**
     * 待确认
     */
    String DEFECT_CONFIRMING = "defectConfirm";
    /**
     * 已完成
     */
    String DEFECT_FINISHED = "finished";
    /**
     * 已放弃
     */
    String DEFECT_ABORT = "giveup";
    /**
     * 今日已消除
     */
    String DEFECT_TODAY_FINISHED = "today_finished";


    //Operation
    /**
     * 提交
     */
    String SUBMIT = "submit";
    /**
     * 交接
     */
    String TAKEOVER = "takeover";
    /**
     * 退回
     */
    String SENDBACK = "back";


    //巡检流程状态
    /**
     * 未分配
     */
    String INSPECT_CREATE = "createInspect";
    /**
     * 未开启
     */
    String INSPECT_START = "startInspect";
    /**
     * 巡检中
     */
    String INSPECT_EXCUTE = "excuteInspect";
    /**
     * 待确认
     */
    String INSPECT_CONFIRM = "confirmInspect";
    /**
     * 完结
     */
    String INSPECT_FINISHED = "finished";

}
