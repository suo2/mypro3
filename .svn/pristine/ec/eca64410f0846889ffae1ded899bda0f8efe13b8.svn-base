<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/common_white"
    tools:context="com.huawei.solarsafe.view.devicemanagement.GridParameterFragment">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <RelativeLayout
                android:id="@+id/rl_grid_code"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:minHeight="50dp">

                <TextView
                    android:id="@+id/tv_name1"
                    android:layout_width="120dp"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:paddingLeft="15dp"
                    android:text="@string/grid_code"
                    android:textColor="#333333"
                    android:textSize="12sp" />

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_toRightOf="@id/tv_name1"
                    android:layout_weight="1"
                    android:background="@drawable/shape_single_item_left_corner_fill">

                    <com.huawei.solarsafe.utils.MySpinner
                        android:id="@+id/spinner_search_option_netcode"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:background="#eeeeee"
                        android:paddingBottom="5dp"
                        android:paddingLeft="5dp"
                        android:paddingRight="15dp"
                        android:paddingTop="5dp"
                        android:singleLine="true"></com.huawei.solarsafe.utils.MySpinner>

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:paddingLeft="10dp"
                        android:paddingRight="5dp"
                        android:src="@drawable/spinner_down_arrow" />
                </RelativeLayout>
            </RelativeLayout>

            <View
                android:id="@+id/view_grid_code"
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:background="#eeeeee" />

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:visibility="gone">

                <TextView
                    android:id="@+id/tv_name2"
                    android:layout_width="120dp"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:paddingLeft="15dp"
                    android:text="@string/voltage_level"
                    android:textColor="#333333"
                    android:textSize="14sp" />

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_toRightOf="@id/tv_name2"
                    android:layout_weight="1"
                    android:background="@drawable/shape_single_item_left_corner_fill">

                    <com.huawei.solarsafe.utils.MySpinner
                        android:id="@+id/spinner_search_option_dydj"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:background="#eeeeee"
                        android:paddingBottom="5dp"
                        android:paddingLeft="5dp"
                        android:paddingRight="15dp"
                        android:paddingTop="5dp"
                        android:singleLine="true"></com.huawei.solarsafe.utils.MySpinner>

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:paddingLeft="10dp"
                        android:paddingRight="5dp"
                        android:src="@drawable/spinner_down_arrow" />
                </RelativeLayout>
            </RelativeLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:background="#eeeeee"
                android:visibility="gone" />

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:visibility="gone">

                <TextView
                    android:id="@+id/tv_name3"
                    android:layout_width="120dp"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:paddingLeft="15dp"
                    android:text="@string/main_grid_frequency"
                    android:textColor="#333333"
                    android:textSize="14sp" />

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_toRightOf="@id/tv_name3"
                    android:layout_weight="1"
                    android:background="@drawable/shape_single_item_left_corner_fill">

                    <com.huawei.solarsafe.utils.MySpinner
                        android:id="@+id/spinner_search_option_dwpl"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:background="#eeeeee"
                        android:paddingBottom="5dp"
                        android:paddingLeft="5dp"
                        android:paddingRight="15dp"
                        android:paddingTop="5dp"
                        android:singleLine="true"></com.huawei.solarsafe.utils.MySpinner>

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:paddingLeft="10dp"
                        android:paddingRight="5dp"
                        android:src="@drawable/spinner_down_arrow" />
                </RelativeLayout>
            </RelativeLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:background="#eeeeee"
                android:visibility="gone" />

            <RelativeLayout
                android:id="@+id/rl_isolation_setting"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:minHeight="50dp">

                <TextView
                    android:id="@+id/tv_name2_1"
                    android:layout_width="120dp"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:paddingLeft="15dp"
                    android:text="@string/isolation_settings_str"
                    android:textColor="#333333"
                    android:textSize="14sp" />

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_toRightOf="@id/tv_name2_1"
                    android:layout_weight="1"
                    android:background="@drawable/shape_single_item_left_corner_fill">

                    <com.huawei.solarsafe.utils.MySpinner
                        android:id="@+id/spinner_search_option_glsz"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:background="#eeeeee"
                        android:paddingBottom="5dp"
                        android:paddingLeft="5dp"
                        android:paddingRight="15dp"
                        android:paddingTop="5dp"
                        android:singleLine="true"></com.huawei.solarsafe.utils.MySpinner>

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:paddingLeft="10dp"
                        android:paddingRight="5dp"
                        android:src="@drawable/spinner_down_arrow" />
                </RelativeLayout>
            </RelativeLayout>

            <View
                android:id="@+id/view_isolation_setting"
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:background="#eeeeee" />

            <RelativeLayout
                android:id="@+id/rl_output_method"
                android:layout_width="match_parent"
                android:minHeight="50dp"
                android:layout_height="wrap_content">

                <TextView
                    android:id="@+id/tv_name2_2"
                    android:layout_width="120dp"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:paddingLeft="15dp"
                    android:text="@string/output_method_str"
                    android:textColor="#333333"
                    android:textSize="14sp" />

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_toRightOf="@id/tv_name2_2"
                    android:layout_weight="1"
                    android:background="@drawable/shape_single_item_left_corner_fill">

                    <com.huawei.solarsafe.utils.MySpinner
                        android:id="@+id/spinner_search_option_scfs"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:background="#eeeeee"
                        android:paddingBottom="5dp"
                        android:paddingLeft="5dp"
                        android:paddingRight="15dp"
                        android:paddingTop="5dp"
                        android:singleLine="true"></com.huawei.solarsafe.utils.MySpinner>

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:paddingLeft="10dp"
                        android:paddingRight="5dp"
                        android:src="@drawable/spinner_down_arrow" />
                </RelativeLayout>
            </RelativeLayout>

            <View
                android:id="@+id/view_output_method"
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:background="#eeeeee" />

            <RelativeLayout
                android:id="@+id/rl_pq"
                android:layout_width="match_parent"
                android:minHeight="50dp"
                android:layout_height="wrap_content">

                <TextView
                    android:id="@+id/tv_name2_3"
                    android:layout_width="120dp"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:paddingLeft="15dp"
                    android:text="@string/pq"
                    android:textColor="#333333"
                    android:textSize="14sp" />

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_toRightOf="@id/tv_name2_3"
                    android:layout_weight="1"
                    android:background="@drawable/shape_single_item_left_corner_fill">

                    <com.huawei.solarsafe.utils.MySpinner
                        android:id="@+id/spinner_search_option_pqms"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:background="#eeeeee"
                        android:paddingBottom="5dp"
                        android:paddingLeft="5dp"
                        android:paddingRight="15dp"
                        android:paddingTop="5dp"
                        android:singleLine="true"></com.huawei.solarsafe.utils.MySpinner>

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:paddingLeft="10dp"
                        android:paddingRight="5dp"
                        android:src="@drawable/spinner_down_arrow" />
                </RelativeLayout>
            </RelativeLayout>

            <View
                android:id="@+id/view_pq"
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:background="#eeeeee" />

            <RelativeLayout
                android:id="@+id/rl_automatic_boot"
                android:layout_width="match_parent"
                android:minHeight="50dp"
                android:layout_height="wrap_content">

                <TextView
                    android:id="@+id/tv_name5"
                    android:layout_width="120dp"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:paddingLeft="15dp"
                    android:text="@string/automatic_boot"
                    android:textColor="#333333"
                    android:textSize="12sp" />

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_toRightOf="@id/tv_name5"
                    android:layout_weight="1"
                    android:background="@drawable/shape_single_item_left_corner_fill">

                    <com.huawei.solarsafe.utils.MySpinner
                        android:id="@+id/spinner_search_option_dwgz"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:background="#eeeeee"
                        android:paddingBottom="5dp"
                        android:paddingLeft="5dp"
                        android:paddingRight="15dp"
                        android:paddingTop="5dp"
                        android:singleLine="true"></com.huawei.solarsafe.utils.MySpinner>

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:paddingLeft="10dp"
                        android:paddingRight="5dp"
                        android:src="@drawable/spinner_down_arrow" />
                </RelativeLayout>
            </RelativeLayout>

            <RelativeLayout
                android:layout_marginTop="5dp"
                android:id="@+id/rl_fault_recovery_time"
                android:layout_width="match_parent"
                android:minHeight="50dp"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/tv_name6"
                    android:layout_width="120dp"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:paddingLeft="15dp"
                    android:text="@string/fault_recovery_time"
                    android:textColor="#333333"
                    android:textSize="12sp" />

                <EditText
                    android:id="@+id/ed_gridnet_time"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="8dp"
                    android:layout_toRightOf="@id/tv_name6"
                    android:background="@drawable/shape_single_item_four_corner_empty"
                    android:hint="@string/please_enter_recovery_time"
                    android:inputType="number"
                    android:padding="8dp"
                    android:singleLine="true"
                    android:textColor="@color/black"
                    android:textSize="12sp" />
            </RelativeLayout>

            <RelativeLayout
                android:layout_marginTop="5dp"
                android:id="@+id/rl_limit_of_reconnection_voltage"
                android:layout_width="match_parent"
                android:minHeight="50dp"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/tv_name7"
                    android:layout_width="120dp"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:paddingLeft="15dp"
                    android:text="@string/limit_of_reconnection_voltage"
                    android:textColor="#333333"
                    android:textSize="12sp" />

                <EditText
                    android:id="@+id/ed_dysx"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="8dp"
                    android:layout_toRightOf="@id/tv_name7"
                    android:background="@drawable/shape_single_item_four_corner_empty"
                    android:digits="0123456789."
                    android:hint="@string/please_select_the_symbol_code"
                    android:inputType="number"
                    android:padding="8dp"
                    android:singleLine="true"
                    android:textColor="@color/black"
                    android:textSize="12sp" />
            </RelativeLayout>

            <RelativeLayout
                android:layout_marginTop="5dp"
                android:id="@+id/rl_lower_limit_of_reconnection_voltage"
                android:layout_width="match_parent"
                android:minHeight="50dp"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/tv_name8"
                    android:layout_width="120dp"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:paddingLeft="15dp"
                    android:text="@string/lower_limit_of_reconnection_voltage"
                    android:textColor="#333333"
                    android:textSize="12sp" />

                <EditText
                    android:id="@+id/ed_dyxx"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="8dp"
                    android:layout_toRightOf="@id/tv_name8"
                    android:background="@drawable/shape_single_item_four_corner_empty"
                    android:digits="0123456789."
                    android:hint="@string/please_select_the_symbol_code"
                    android:inputType="number"
                    android:padding="8dp"
                    android:singleLine="true"
                    android:textColor="@color/black"
                    android:textSize="12sp" />
            </RelativeLayout>

            <RelativeLayout
                android:layout_marginTop="5dp"
                android:id="@+id/rl_upper_limit_of_reconnection"
                android:layout_width="match_parent"
                android:minHeight="50dp"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/tv_name9"
                    android:layout_width="120dp"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:paddingLeft="15dp"
                    android:text="@string/upper_limit_of_reconnection"
                    android:textColor="#333333"
                    android:textSize="12sp" />

                <EditText
                    android:id="@+id/ed_plsx"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="8dp"
                    android:layout_toRightOf="@id/tv_name9"
                    android:background="@drawable/shape_single_item_four_corner_empty"
                    android:digits="0123456789."
                    android:hint="@string/please_select_the_symbol_code"
                    android:inputType="number"
                    android:padding="8dp"
                    android:singleLine="true"
                    android:textColor="@color/black"
                    android:textSize="12sp" />
            </RelativeLayout>

            <RelativeLayout
                android:layout_marginTop="5dp"
                android:id="@+id/rl_lower_limit_of_reconnection"
                android:layout_width="match_parent"
                android:minHeight="50dp"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/tv_name10"
                    android:layout_width="120dp"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:paddingLeft="15dp"
                    android:text="@string/lower_limit_of_reconnection"
                    android:textColor="#333333"
                    android:textSize="12sp" />

                <EditText
                    android:id="@+id/ed_plxx"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_alignTop="@+id/tv_name10"
                    android:layout_marginRight="8dp"
                    android:layout_toEndOf="@+id/tv_name10"
                    android:layout_toRightOf="@+id/tv_name10"
                    android:background="@drawable/shape_single_item_four_corner_empty"
                    android:digits="0123456789."
                    android:hint="@string/please_select_the_symbol_code"
                    android:inputType="number"
                    android:padding="8dp"
                    android:singleLine="true"
                    android:textColor="@color/black"
                    android:textSize="12sp" />
            </RelativeLayout>

            <RelativeLayout
                android:layout_marginTop="5dp"
                android:id="@+id/rl_reactive_power_compensation_cf_v_str"
                android:layout_width="match_parent"
                android:minHeight="50dp"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/tv_name11"
                    android:layout_width="120dp"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:paddingLeft="15dp"
                    android:text="@string/reactive_power_compensation_cf_v_str"
                    android:textColor="#333333"
                    android:textSize="12sp" />

                <EditText
                    android:id="@+id/ed_wgbccfdy"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="8dp"
                    android:layout_toRightOf="@id/tv_name11"
                    android:background="@drawable/shape_single_item_four_corner_empty"
                    android:digits="0123456789."
                    android:inputType="number"
                    android:padding="8dp"
                    android:singleLine="true"
                    android:textColor="@color/black"
                    android:textSize="12sp" />
            </RelativeLayout>

            <RelativeLayout
                android:layout_marginTop="5dp"
                android:id="@+id/rl_reactive_power_compensation_tc_v_str"
                android:layout_width="match_parent"
                android:minHeight="50dp"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/tv_name12"
                    android:layout_width="120dp"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:paddingLeft="15dp"
                    android:text="@string/reactive_power_compensation_tc_v_str"
                    android:textColor="#333333"
                    android:textSize="12sp" />

                <EditText
                    android:id="@+id/ed_wgbctcdy"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="8dp"
                    android:layout_toRightOf="@id/tv_name12"
                    android:background="@drawable/shape_single_item_four_corner_empty"
                    android:digits="0123456789."
                    android:inputType="number"
                    android:padding="8dp"
                    android:singleLine="true"
                    android:textColor="@color/black"
                    android:textSize="12sp" />
            </RelativeLayout>
        </LinearLayout>
    </ScrollView>

</LinearLayout>
