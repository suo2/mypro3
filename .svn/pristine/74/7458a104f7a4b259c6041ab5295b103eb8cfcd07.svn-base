                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="11"
                            android:gravity="right"
                            android:text="@string/temperature_inside_the_box"
                            android:textColor="#333333"
                            android:textSize="12sp" />

